{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/skills/search",
      "destination": "https://api.claw.getfoundry.app/skills/search"
    },
    {
      "source": "/skills/publish",
      "destination": "https://api.claw.getfoundry.app/skills/publish"
    },
    {
      "source": "/skills/:id/summary",
      "destination": "https://api.claw.getfoundry.app/skills/:id/summary"
    },
    {
      "source": "/skills/:id/download",
      "destination": "https://api.claw.getfoundry.app/skills/:id/download"
    },
    {
      "source": "/abilities/:path*",
      "destination": "https://api.claw.getfoundry.app/abilities/:path*"
    },
    {
      "source": "/health",
      "destination": "https://api.claw.getfoundry.app/health"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
